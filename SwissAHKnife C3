Account := Object()
NumAccs:=0
Title:=
AccCounter:=0
NotExistCorrection:=0

AccountRead:
ifExist %A_ScriptDir%\SerbzOSD\NumAccs.log
{
    FileRead, NumAccs, %A_ScriptDir%\SerbzOSD\NumAccs.log
    
while (AccCounter<NumAccs) {
    AccCounter++
        ifExist %A_ScriptDir%\SerbzOSD\Account%AccCounter%.log
        {
            FileRead, AccTempAdd, %A_ScriptDir%\SerbzOSD\Account%AccCounter%.log
            Account.Insert(AccTempAdd)
        }
    }
} 
        
IfExist %A_ScriptDir%\SerbzOSD\NumAccs.log
    FileDelete, %A_ScriptDir%\SerbzOSD\NumAccs.log
FileAppend, %NumAccs%, %A_ScriptDir%\SerbzOSD\NumAccs.log
;-------------------------------------;-------------------------------------;-------------------------------------;-------------------------------------;-------------------------------------;-------------------------------------;;-;-;-;-;-;-;-;-
;-;-;-;-;-;-;-;-;-;-;-; Check for runelite directory  ;-;-;-;-;-;-;-;-;-;-;-;-;-;-;-
;-------------------------------------;-------------------------------------;-------------------------------------;-------------------------------------;-------------------------------------;-------------------------------------;;-;-;-;-;-;-;-;-
ifNotExist, %A_ScriptDir%\SerbzOSD\RuneLiteDir.log 
{
    SetTimer, MsgboxMove, on
    MsgboxMove_String=Browse for Runelite
    MsgBox, 4, %MsgboxMove_String%, Browse to runelite.exe?
    IfMsgBox, Yes 
    {
        FileSelectFile, RuneLiteDir
        FileAppend, %RuneLiteDir%, %A_ScriptDir%\SerbzOSD\RuneLiteDir.log
    }
    IfMsgBox, No
    {
        NoRLDir:=1
    }
    goto AccPub2
} else {
    FileRead, RuneLiteDir, %A_ScriptDir%\SerbzOSD\RuneLiteDir.log
    goto AccPub2
}
AccPub2:
If (RuneLiteDir:=) {
    NoRLDir:=1
}
