    ChaosY[CounterWinBack2]:=((ChaosArray[CounterWinBack3-5] + ChaosArray[CounterWinBack3-1]/2)-WinNameNH[CounterWinBack2]/2)

    WinMove, ahk_ID %AHKCHAOSID% , , % ChaosX[CounterWinBack2], % ChaosY[CounterWinBack2]
    RandyRand:=0
