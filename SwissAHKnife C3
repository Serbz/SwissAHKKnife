                AccTimerTSec.RemoveAt(CapsTAccTimersCounter)
                AccTimerTSecMin.RemoveAt(CapsTAccTimersCounter)
                AccTimerFTMin.RemoveAt(CapsTAccTimersCounter)
                AccTimerCDSec.RemoveAt(CapsTAccTimersCounter)

