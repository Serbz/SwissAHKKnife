setTimer, RandomVolForAssholes, %var%
SoundSetWaveVolume, Rand(100)
return

AlarmSound:
alcounter+=1
if (AltAlarmSound=1){
AlarmDir=%A_WinDir%\Media\Alarm07.wav
SoundSetWaveVolume, 50
} else {
AlarmDir = %A_WinDir%\Media\Alarm10.wav
}
if (PlaySound=1) {
    SoundPlay, %AlarmDir%, wait
} else {
    PlaySound:=2
	alcounter:=0
	SoundPlay, Nonexistent.avi
    SetTimer, AlarmSound, off    
}
if (AltAlarmSound=1 and alcounter>0){
	AltAlarmSound:=0
	alcounter:=0
	SoundSetWaveVolume, 100
	SoundPlay, Nonexistent.avi
	SetTimer, AlarmSound, off  
}
return
